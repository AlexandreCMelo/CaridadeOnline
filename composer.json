{
  "name": "alexandre-cmelo/caridade-online",
  "description": "Caridade Online, fa\u00e7a a diferen\u00e7a : )",
  "keywords": [
    "charity",
    "charitable",
    "caridade",
    "ajudar"
  ],
  "license": "GPL v3",
  "authors": [
    {
      "name": "Alexandre Melo",
      "email": "alexandrecarvalhodemelo@outlook.com",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=7.0.0",
    "alexpechkarev/google-maps": "1.0.7",
    "aws/aws-sdk-php": "^3.36",
    "cmgmyr/messenger": "^2.14",
    "fideloper/proxy": "~3.3",
    "guzzlehttp/guzzle": "^6.3",
    "laravel/framework": "5.5.*",
    "laravel/socialite": "^3.0",
    "laravel/tinker": "~1.0",
    "laravelcollective/html": "^5.5",
    "pusher/pusher-php-server": "~3.0",
    "socialiteproviders/generators": "^2.0",
    "socialiteproviders/instagram": "^3.0",
    "socialiteproviders/linkedin": "^3.0",
    "socialiteproviders/reddit": "^3.0",
    "symfony/dom-crawler": "^3.3",
    "symfony/psr-http-message-bridge": "^1.0"
  },
  "require-dev": {
    "filp/whoops": "~2.0",
    "fzaninotto/faker": "~1.7",
    "mockery/mockery": "0.9.*",
    "phpunit/phpunit": "~6.3"
  },
  "autoload": {
    "classmap": [
      "database/seeds",
      "database/factories"
    ],
    "psr-4": {
      "Charis\\": "app/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Illuminate\\Support\\ServiceProvider\\ImageServiceProviderLaravel5"
      ],
      "dont-discover": []
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r file_exists('.env') || copy('.env.example', '.env');"
    ],
    "post-create-project-cmd": [
      "php artisan key:generate"
    ],
    "post-install-cmd": [
      "Illuminate\\Foundation\\ComposerScripts::postInstall",
      "php artisan optimize"
    ],
    "post-update-cmd": [
      "Illuminate\\Foundation\\ComposerScripts::postUpdate",
      "php artisan optimize"
    ]
  },
    "config": {
      "preferred-install": "dist",
      "sort-packages": true,
      "optimize-autoloader": true
    }
  }
